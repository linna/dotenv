{
    "name": "linna/doenv",
    "type": "library",
    "description": "Linna .env tool",
    "keywords": [".env", "env", "php"],
    "homepage": "https://github.com/linna/dotenv",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Rapetti",
            "email": "sebastian.rapetti@alice.it"
        }
    ],
    "support": {
        "issues": "https://github.com/linna/dotenv/issues"
    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "infection/infection": "^0.10",
        "phpstan/phpstan": "^0.10",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Linna\\DotEnv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linna\\Tests\\": "tests/"
        }
    }
}
