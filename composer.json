{
    "name": "linna/dotenv",
    "type": "library",
    "description": "Linna .env tool",
    "keywords": [".env", "env", "php"],
    "homepage": "https://github.com/linna/dotenv",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Rapetti",
            "email": "sebastian.rapetti@tim.it"
        }
    ],
    "support": {
        "issues": "https://github.com/linna/dotenv/issues"
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "infection/infection": ">=0.26",
        "phpstan/phpstan": ">=1.4",
        "phpunit/phpunit": ">=9.0"
    },
    "autoload": {
        "psr-4": {
            "Linna\\DotEnv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linna\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ocramius/package-versions": true,
            "infection/extension-installer": true
        }
    }
}
